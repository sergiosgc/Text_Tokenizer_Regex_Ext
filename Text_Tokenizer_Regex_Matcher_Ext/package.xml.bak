<?xml version="1.0"?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package>

  <name>Text_Tokenizer_Regex_Matcher_Ext</name>
  <summary>Text_Tokenizer_Regex_Matcher_Ext is a regexp matcher to be used wit Text_Tokenizer_Regex</summary>
  <description>
Text_Tokenizer_Regex_Matcher_Ext performs just like the Text_Tokenizer_Regex_Matcher_Php bundled with Text_Tokenizer_Regex, only faster
  </description>

  <license>PHP</license>

  <configureoptions>
   <configureoption name="with-pcre" default="autodetect" prompt="pcre installation directory?" />
  </configureoptions>

  <maintainers>
    <maintainer>
      <user>sergiosgc</user>
      <name>Sergio Carvalho</name>
      <email>sergiosgc@pear.php.net</email>
      <role>lead</role>
    </maintainer>
  </maintainers>

  <release>
    <version>1.0</version>
    <state>stable</state>
    <notes>First public release</notes>
    <date>2007-12-14</date>
  </release>
  <changelog>

  </changelog>
  <deps>
    <dep type="php" rel="ge" version="5.0.0"/>
  </deps>

  <filelist>
    <dir name="/">
      <file role='doc' name='CREDITS'/>
      <file role='src' name='config.m4'/>
      <file role='src' name='Text_Tokenizer_Regex_Matcher_Ext.dsp'/>
      <file role='src' name='config.w32'/>
      <file role='src' name='Text_Tokenizer_Regex_Matcher_Ext.c'/>
      <file role='src' name='php_Text_Tokenizer_Regex_Matcher_Ext.h'/>
      <dir name="tests">
        <file role='test' name='Text_Tokenizer_Regex_Matcher_Ext__setInput.phpt'/>
        <file role='test' name='Text_Tokenizer_Regex_Matcher_Ext__setSelectionCriteria.phpt'/>
        <file role='test' name='Text_Tokenizer_Regex_Matcher_Ext__addRegex.phpt'/>
        <file role='test' name='Text_Tokenizer_Regex_Matcher_Ext__match.phpt'/>
        <file role='test' name='Text_Tokenizer_Regex_Matcher_Ext__consume.phpt'/>
      </dir>
    </dir>
  </filelist>
</package>
